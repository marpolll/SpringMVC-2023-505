<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.marpol.calendar.dao.CalendarDao">

	<insert id="insert">
		insert into calendar(c_id, events) 
		values
		(#{c_id},'{ "title" : "#{title}", "start" : "#{start}", "end": "#{end}" }');
	</insert>

</mapper>